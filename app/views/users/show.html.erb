<h2>Profile</h2>
<div class="row">
   <div class="col-md-8">
     <div class="media">
       <div class="media-body">
         <h5 class="media-heading">Name: <%= @user.name %></h5>
         <h5 class="media-heading">Email: <%= @user.email %></h5>
       </div>
     </div>
   </div>
   <div class="col-md-4">
      <%= link_to "Edit Account", edit_user_registration_path, class: 'btn btn-success' %>
   </div>
 </div>
 <div class="row">
   <div class="col-md-8">
      <h2>My Items</h2>
         <% @user.items.each do |item| %>
            <%= render partial: 'items/item', locals: {item: item} %>
         <% end %>
   </div>
 </div>
 </br>
 
