<h3><%= @item.name %></h3>
 <div class="row">
   <div class="col-md-8">
      <%= render partial: 'items/item', locals: { item: @item } %>
   </div>
   <div class="col-md-4">
    <% if current_user %>
      <%= link_to "Edit Item", edit_item_path, class: 'btn btn-success' %>
    <% end %>
   </div>
 </div>